<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean class="in.sp.bean.Address" id="address">
	<property name="houseno" value="5311"></property>
	<property name="city" value="hyderabad"></property>
	<property name="pincode" value="500045"></property>
	</bean>
    <bean class="in.sp.bean.Student" id="stdId" autowire="byName">
    
    <property name="rollno" value="7423"></property>
    <property name="name" value="Bhanu"></property>
    
    </bean>

</beans>